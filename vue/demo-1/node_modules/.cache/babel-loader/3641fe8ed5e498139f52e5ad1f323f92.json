le=\"background: #545c64; width: 100vh \" >\r\n    <!--    功能区域-->\r\n    <div style=\"margin: 5px 10px \">\r\n      <el-upload :action=\"'http://' + serverIp + ':'+serverPort+'/file/upload'\" :show-file-list=\"false\"\r\n                 :on-success=\"handleFileUploadSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\">上传文件 <i class=\"el-icon-top\"></i></el-button>\r\n      </el-upload>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n    </div>\r\n    <!--    搜索区域-->\r\n    <div style=\"margin: 5px 10px\" >\r\n      <el-input v-model=\"search\" placeholder=\"Please input\" clearable style=\"width: 40vh\">\r\n        <template #append >\r\n          <el-button type=\"primary\" @click=\"load\">\r\n            <el-icon >\r\n              <Search />\r\n            </el-icon>\r\n            查询\r\n          </el-button>\r\n        </template>\r\n      </el-input>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <!--    主体-->\r\n    <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        max-height=\"700\"\r\n        stripe\r\n        style=\"width:99%;margin: 10px\">\r\n      <el-table-column\r\n          type=\"selection\"\r\n          header-align=\"center\"\r\n          align=\"center\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"id\"\r\n          header-align=\"center\"\r\n          align=\"center\"\r\n          label=\"ID\"\r\n          width=\"80\"\r\n          sortable\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"name\"\r\n          header-align=\"center\"\r\n          align=\"left\"\r\n          sortable\r\n          label=\"文件名称\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"type\"\r\n          header-align=\"center\"\r\n          sortable\r\n          align=\"center\"\r\n          label=\"类型\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n          prop=\"size\"\r\n          header-align=\"center\"\r\n          sortable\r\n          align=\"left\"\r\n          label=\"文件大小\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column label=\"预览\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"preview(scope.row.url)\">预览</el-button>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"下载\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"download(scope.row.url)\">下载</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--    分页-->\r\n    <div style=\"margin: 10px 0;text-align: center\">\r\n\r\n      <el-pagination class=\"el-pagination_Color\"\r\n                     background\r\n                     @size-change=\"handleSizeChange\"\r\n                     @current-change=\"handleCurrentChange\"\r\n                     :current-page=\"currentPage\"\r\n                     :page-sizes=\"[10, 20, 50]\"\r\n                     :page-size=\"pageSize\"\r\n                     layout=\"total, sizes, prev, pager, next, jumper\"\r\n                     :total=\"total\"\r\n\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport {serverIp, serverPort} from \"../../public/config\";\r\nimport Search from \"@element-plus/icons/lib/Search\";\r\n\r\nexport default {\r\n  name: \"FileInfo\",\r\n  components: {\r\n    Search\r\n  },\r\n  data(){\r\n\r\n    return{\r\n      serverIp: serverIp,\r\n      serverPort: serverPort,\r\n      loading: true,\r\n      multipleSelection: [],\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogVisible:false,\r\n      search:'',\r\n      tableData:[ ],\r\n\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods:{\r\n    load(){\r\n      request.get(\"/files/info\",{\r\n        params:{\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          search: this.search\r\n\r\n        }}).then(res => {\r\n        // console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    changeEnable(row) {\r\n      this.request.post(\"/file/update\", row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n        }\r\n      })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/files/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    save(){\r\n\r\n      if(this.user_form.id){\r\n        request.put(\"/user/user_info\",this.user_form).then(res=>{\r\n          // console.log(res)\r\n          if(res.code=='200'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n            this.load()\r\n            this.dialogVisible=false\r\n          }\r\n          else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:\"更新失败\"\r\n            })\r\n          }\r\n        })\r\n      }\r\n      else {\r\n        request.post(\"/user/user_info\",this.user_form).then(res=>{\r\n          // console.log(res)\r\n          if(res.code=='200'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"添加成功\"\r\n\r\n            })\r\n            this.load()\r\n            this.dialogVisible=false\r\n          }\r\n          else {\r\n            this.$message({\r\n              type:\"error\",\r\n              message:\"添加失败\"\r\n\r\n            })\r\n          }\r\n        })\r\n      }\r\n\r\n    },\r\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.el-pagination_Color\r\n/deep/ .el-pagination__total {\r\n  color: #ffffff;\r\n}\r\n/deep/ .el-pagination__jump {\r\n  color: #ffffff;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}